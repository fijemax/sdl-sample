```
sudo apt install libpng-dev libjpeg9-dev libsdl2-dev libsdl2-image-dev
mkdir build && cd build
cmake .. -DCMAKE_INSTALL_PREFIX=../install
make -j4
make install
cd ../install/bin
./SDL-Test

gcc trace
-DCMAKE_VERBOSE_MAKEFILE=on